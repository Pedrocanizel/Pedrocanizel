from sklearn.model_selection import RandomizedSearchCV

é uma forma de fazer testes com meus modelos de forma automatizada



# Definindo os valores que serão testados
valores = {'alpha': [0.1, 0.2, 0.5, 0.8, 1, 2, 5 10], 'l1_ratio': [0.02, 0.05, 0.1, 0.2]}

modelo = ElasticNet()
procura = RandomizedSearchCV(estimator =  modelo, param_distributions=valores, n_iter=150, cv=5, random_state=15)
procura.fit(x, y)






















